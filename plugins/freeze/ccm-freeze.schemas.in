<?xml version="1.0"?>
<gconfschemafile>
   <schemalist>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/freeze/delay</key>
         <applyto>/apps/cairo-compmgr/default/freeze/delay</applyto>
         <owner>gnome</owner>
         <type>int</type>
	 <default>5</default>
         <locale name="C">
            <short>Freeze ping delay</short>
            <long>Freeze ping delay in seconds</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/freeze/duration</key>
         <applyto>/apps/cairo-compmgr/default/freeze/duration</applyto>
         <owner>gnome</owner>
         <type>float</type>
	 <default>0.3</default>
         <locale name="C">
            <short>Freeze animation duration</short>
            <long>Freeze animation duration</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/freeze/color</key>
         <applyto>/apps/cairo-compmgr/default/freeze/color</applyto>
         <owner>gnome</owner>
         <type>string</type>
	 <default>#000000</default>
         <locale name="C">
            <short>Freeze color</short>
            <long>Freeze color</long>
         </locale>
      </schema>
   </schemalist>
</gconfschemafile>
