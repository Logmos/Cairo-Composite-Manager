<?xml version="1.0"?>
<gconfschemafile>
   <schemalist>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/opacity/opacity</key>
         <applyto>/apps/cairo-compmgr/default/opacity/opacity</applyto>
         <owner>gnome</owner>
         <type>float</type>
		 <default>0.85</default>
         <locale name="C">
            <short>Opacity of menu</short>
            <long>Opacity of menu</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/opacity/increase</key>
         <applyto>/apps/cairo-compmgr/default/opacity/increase</applyto>
         <owner>gnome</owner>
         <type>string</type>
		 <default>&lt;Super&gt;Button4</default>
         <locale name="C">
            <short>Increase opacity shortcut key.</short>
            <long>The increase opacity shortcut key.</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/opacity/decrease</key>
         <applyto>/apps/cairo-compmgr/default/opacity/decrease</applyto>
         <owner>gnome</owner>
         <type>string</type>
		 <default>&lt;Super&gt;Button5</default>
         <locale name="C">
            <short>Decrease opacity shortcut key.</short>
            <long>The decrease opacity shortcut key.</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/opacity/step</key>
         <applyto>/apps/cairo-compmgr/default/opacity/step</applyto>
         <owner>gnome</owner>
         <type>float</type>
		 <default>0.1</default>
         <locale name="C">
            <short>Opacity step on increase/decrease</short>
            <long>Opacity step on increase/decrease</long>
         </locale>
      </schema>
   </schemalist>
</gconfschemafile>
