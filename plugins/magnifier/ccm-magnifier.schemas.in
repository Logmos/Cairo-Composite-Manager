<?xml version="1.0"?>
<gconfschemafile>
   <schemalist>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/magnifier/enable</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/enable</applyto>
         <owner>gnome</owner>
         <type>bool</type>
		 <default>false</default>
         <locale name="C">
            <short>Magnifier activation.</short>
            <long>Activate/desactivate the magnifier.</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/magnifier/zoom-level</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/zoom-level</applyto>
         <owner>gnome</owner>
         <type>float</type>
		 <default>1.5</default>
         <locale name="C">
            <short>Magnifier zoom level.</short>
            <long>The magnifier zoom level determines how much the screen is magnified.</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/magnifier/zoom-quality</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/zoom-quality</applyto>
         <owner>gnome</owner>
         <type>string</type>
		 <default>good</default>
         <locale name="C">
            <short>Magnifier zoom quality.</short>
            <long>The magnifier zoom quality determines zoom filter quality (fast, good, best).</long>
         </locale>
      </schema>
      <schema>
	 <key>/schemas/apps/cairo-compmgr/default/magnifier/x</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/x</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>-1</default>
         <locale name="C">
            <short>Magnifier x position.</short>
            <long>Magnifier x position negative value center magnifier in the middle of screen.</long>
         </locale>
      </schema>
     <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/y</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/y</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>-1</default>
         <locale name="C">
            <short>Magnifier y position.</short>
            <long>Magnifier y position negative value center magnifier in the middle of screen.</long>
         </locale>
      </schema>
     <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/height</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/height</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>80</default>
         <locale name="C">
            <short>Magnifier height in percent.</short>
            <long>Magnifier height in percent of screen height.</long>
         </locale>
      </schema>
      <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/width</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/width</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>80</default>
         <locale name="C">
            <short>Magnifier width in percent.</short>
            <long>Magnifier width in percent of screen width.</long>
         </locale>
      </schema>
      <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/restrict_area_x</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/restrict_area_x</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>-1</default>
         <locale name="C">
            <short>Magnifier restrict area x position.</short>
            <long>Magnifier restrict area x position negative value get screen.</long>
         </locale>
      </schema>
     <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/restrict_area_y</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/restrict_area_y</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>-1</default>
         <locale name="C">
            <short>Magnifier restrict area y position.</short>
            <long>Magnifier restrict area y position negative get screen.</long>
         </locale>
      </schema>
     <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/restrict_area_height</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/restrict_area_height</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>-1</default>
         <locale name="C">
            <short>Magnifier restrict area height.</short>
            <long>Magnifier restrict area height negative value get screen height.</long>
         </locale>
      </schema>
      <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/restrict_area_width</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/restrict_area_width</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>-1</default>
         <locale name="C">
            <short>Magnifier restrict area width.</short>
            <long>Magnifier restrict area width negative value get screen width.</long>
         </locale>
      </schema>
      <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/border</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/border</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>20</default>
         <locale name="C">
            <short>Border size before move view.</short>
            <long>Border size before move view.</long>
         </locale>
      </schema>
      <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/shortcut</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/shortcut</applyto>
         <owner>gnome</owner>
         <type>string</type>
		 <default>&lt;Super&gt;F12</default>
         <locale name="C">
            <short>Magnifier shortcut key.</short>
            <long>The magnifier shortcut key.</long>
         </locale>
      </schema>
      <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/shade_desktop</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/shade_desktop</applyto>
         <owner>gnome</owner>
         <type>bool</type>
		 <default>true</default>
         <locale name="C">
            <short>Magnifier shade desktop.</short>
            <long>Shade desktop when magnifier is enabled.</long>
         </locale>
      </schema>
   </schemalist>
</gconfschemafile>
