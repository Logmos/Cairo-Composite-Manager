<?xml version="1.0"?>
<gconfschemafile>
   <schemalist>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/magnifier/zoom-level</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/zoom-level</applyto>
         <owner>gnome</owner>
         <type>float</type>
		 <default>2</default>
         <locale name="C">
            <short>Magnifier zoom level.</short>
            <long>The magnifier zoom level determines how much the screen is magnified.</long>
         </locale>
      </schema>
      <schema>
	 <key>/schemas/apps/cairo-compmgr/default/magnifier/height</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/height</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>70</default>
         <locale name="C">
            <short>Magnifier height in percent.</short>
            <long>Magnifier height in percent of screen height.</long>
         </locale>
      </schema>
      <schema>
	 <key>/schemas/apps/cairo-compmgr/default/magnifier/width</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/width</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>70</default>
         <locale name="C">
            <short>Magnifier width in percent.</short>
            <long>Magnifier width in percent of screen width.</long>
         </locale>
      </schema>
      <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/shortcut</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/shortcut</applyto>
         <owner>gnome</owner>
         <type>string</type>
		 <default>F12</default>
         <locale name="C">
            <short>Magnifier shortcut key.</short>
            <long>The magnifier shortcut key.</long>
         </locale>
      </schema>
      <schema>
      <key>/schemas/apps/cairo-compmgr/default/magnifier/shade_desktop</key>
         <applyto>/apps/cairo-compmgr/default/magnifier/shade_desktop</applyto>
         <owner>gnome</owner>
         <type>bool</type>
		 <default>true</default>
         <locale name="C">
            <short>Magnifier shade desktop.</short>
            <long>Shade desktop when magnifier is enabled.</long>
         </locale>
      </schema>
   </schemalist>
</gconfschemafile>
