<?xml version="1.0"?>
<gconfschemafile>
   <schemalist>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/mosaic/spacing</key>
         <applyto>/apps/cairo-compmgr/default/mosaic/spacing</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>20</default>
         <locale name="C">
            <short>Mosaic spacing.</short>
            <long>The spacing between each areas of mosaic.</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/mosaic/shortcut</key>
         <applyto>/apps/cairo-compmgr/default/mosaic/shortcut</applyto>
         <owner>gnome</owner>
         <type>string</type>
		 <default>&lt;Super&gt;Tab</default>
         <locale name="C">
            <short>Mosaic shortcut key.</short>
            <long>The mosaic shortcut key.</long>
         </locale>
      </schema>
   </schemalist>
</gconfschemafile>
