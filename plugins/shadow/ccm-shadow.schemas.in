<?xml version="1.0"?>
<gconfschemafile>
   <schemalist>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/shadow/border</key>
         <applyto>/apps/cairo-compmgr/default/shadow/border</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>14</default>
         <locale name="C">
            <short>Size of shadow</short>
            <long>Size of shadow in pixel</long>
         </locale>
      </schema>
	  <schema>
         <key>/schemas/apps/cairo-compmgr/default/shadow/radius</key>
         <applyto>/apps/cairo-compmgr/default/shadow/radius</applyto>
         <owner>gnome</owner>
         <type>int</type>
		 <default>8</default>
         <locale name="C">
            <short>Shadow blur radius</short>
            <long>Shadow blur radius</long>
         </locale>
      </schema>
      <schema>
         <key>/schemas/apps/cairo-compmgr/default/shadow/color</key>
         <applyto>/apps/cairo-compmgr/default/shadow/color</applyto>
         <owner>gnome</owner>
         <type>string</type>
		 <default>#767676</default>
         <locale name="C">
            <short>Shadow color</short>
            <long>Shadow color</long>
         </locale>
      </schema>
   </schemalist>
</gconfschemafile>
